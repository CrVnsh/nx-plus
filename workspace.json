{
  "version": 1,
  "projects": {
    "docusaurus": {
      "root": "libs/docusaurus",
      "sourceRoot": "libs/docusaurus/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/docusaurus/.eslintrc",
            "tsConfig": [
              "libs/docusaurus/tsconfig.lib.json",
              "libs/docusaurus/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/docusaurus/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/docusaurus/jest.config.js",
            "tsConfig": "libs/docusaurus/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/docusaurus",
            "tsConfig": "libs/docusaurus/tsconfig.lib.json",
            "packageJson": "libs/docusaurus/package.json",
            "main": "libs/docusaurus/src/index.ts",
            "assets": [
              "libs/docusaurus/*.md",
              {
                "input": "./libs/docusaurus/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/docusaurus",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/docusaurus",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "docusaurus-e2e": {
      "projectType": "application",
      "root": "apps/docusaurus-e2e",
      "sourceRoot": "apps/docusaurus-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "docusaurus:build",
            "npmPackageName": "@nx-plus/docusaurus",
            "pluginOutputPath": "dist/libs/docusaurus",
            "jestConfig": "apps/docusaurus-e2e/jest.config.js",
            "tsSpecConfig": "apps/docusaurus-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "vue-plugin": {
      "root": "libs/vue",
      "sourceRoot": "libs/vue/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/vue/.eslintrc",
            "tsConfig": [
              "libs/vue/tsconfig.lib.json",
              "libs/vue/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/vue/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/vue/jest.config.js",
            "tsConfig": "libs/vue/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/vue",
            "tsConfig": "libs/vue/tsconfig.lib.json",
            "packageJson": "libs/vue/package.json",
            "main": "libs/vue/src/index.ts",
            "assets": [
              "libs/vue/*.md",
              {
                "input": "./libs/vue/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/vue",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/vue",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "vue-plugin-e2e": {
      "projectType": "application",
      "root": "apps/vue-e2e",
      "sourceRoot": "apps/vue-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "vue-plugin:build",
            "npmPackageName": "@nx-plus/vue",
            "pluginOutputPath": "dist/libs/vue",
            "jestConfig": "apps/vue-e2e/jest.config.js",
            "tsSpecConfig": "apps/vue-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "my-vue-app": {
      "projectType": "application",
      "root": "apps/my-vue-app",
      "sourceRoot": "apps/my-vue-app/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/vue:browser",
          "options": {
            "outputPath": "dist/apps/my-vue-app",
            "index": "apps/my-vue-app/src/index.html",
            "main": "apps/my-vue-app/src/main.ts",
            "tsConfig": "apps/my-vue-app/tsconfig.app.json",
            "assets": [
              "apps/my-vue-app/src/favicon.ico",
              "apps/my-vue-app/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/my-vue-app/src/environments/environment.ts",
                  "with": "apps/my-vue-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "extractCss": true
            }
          }
        },
        "serve": {
          "builder": "@nx-plus/vue:dev-server",
          "options": {
            "browserTarget": "my-vue-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "my-vue-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/my-vue-app/.eslintrc",
            "tsConfig": [
              "apps/my-vue-app/tsconfig.app.json",
              "apps/my-vue-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/my-vue-app/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/my-vue-app/jest.config.js",
            "tsConfig": "apps/my-vue-app/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-vue-app-e2e": {
      "root": "apps/my-vue-app-e2e",
      "sourceRoot": "apps/my-vue-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-vue-app-e2e/cypress.json",
            "tsConfig": "apps/my-vue-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "my-vue-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-vue-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/my-vue-app-e2e/.eslintrc",
            "tsConfig": ["apps/my-vue-app-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/my-vue-app-e2e/**"]
          }
        }
      }
    },
    "admin": {
      "projectType": "application",
      "root": "apps/admin",
      "sourceRoot": "apps/admin/src",
      "architect": {
        "build": {
          "builder": "@nx-plus/vue:browser",
          "options": {
            "outputPath": "dist/apps/admin",
            "index": "apps/admin/src/index.html",
            "main": "apps/admin/src/main.ts",
            "tsConfig": "apps/admin/tsconfig.app.json",
            "assets": [
              "apps/admin/src/favicon.ico",
              "apps/admin/src/assets",
              {
                "glob": "**/*",
                "input": "src/public/",
                "output": "/public/"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "extractCss": true
            }
          }
        },
        "serve": {
          "builder": "@nx-plus/vue:dev-server",
          "options": {
            "browserTarget": "admin:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "admin:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/admin/.eslintrc",
            "tsConfig": [
              "apps/admin/tsconfig.app.json",
              "apps/admin/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/admin/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/admin/jest.config.js",
            "tsConfig": "apps/admin/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "admin-e2e": {
      "root": "apps/admin-e2e",
      "sourceRoot": "apps/admin-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-e2e/cypress.json",
            "tsConfig": "apps/admin-e2e/tsconfig.e2e.json",
            "devServerTarget": "admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/admin-e2e/.eslintrc",
            "tsConfig": ["apps/admin-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/admin-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  }
}
